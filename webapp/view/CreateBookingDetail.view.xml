<mvc:View controllerName="sap.ui.demo.fiori2.controller.CreateBookingDetail" xmlns="sap.ui.table" xmlns:mvc="sap.ui.core.mvc"
	xmlns:u="sap.ui.unified" xmlns:c="sap.ui.core" xmlns:m="sap.m" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core" height="100%">
	<m:Page showHeader="false" enableScrolling="false" class="sapUiContentPadding">
		<m:content>
			<m:OverflowToolbar class="sapFDynamicPageAlignContent">
				<m:Title id="title" text="Create Booking"/>
			</m:OverflowToolbar>
			<form:SimpleForm minWidth="1024" maxContainerCols="2" layout="ResponsiveLayout" editable="true" visible="true">
				<form:content>
						<m:Label text="Date"/>
						<m:DatePicker dateValue="" change="handleChange" valueFormat="yyyy-MM-dd" placeholder="MMM d. y" id="datade" width="60%"/>
						<m:Label text="Start"/>
						<m:TimePicker id="TP1" value="" valueFormat="HH:mm" displayFormat="HH:mm" change="handleChange" placeholder="HH:mm" width="60%"/>
						<m:Label text="End"/>
						<m:TimePicker id="TP2" value="" valueFormat="HH:mm" displayFormat="HH:mm" change="handleChange" placeholder="HH:mm" width="60%"/>
						<m:Label text="Hours"/>
						<m:Input value="{Erdat}" change="handleChange" width="60%" enabled="false"/>
						<m:Label text="PSP"/>
						<m:ComboBox id="pspComboBox" items="{ path: 'psps>/pspsData' }" width="60%">
							<core:Item key="{psps>pspId}" text="{psps>pspName}" enabled="true"/>
						</m:ComboBox>
						<m:Label text="Description"/>
						<m:Input id="description" value="" change="handleChange" width="60%"/>
				</form:content>
			</form:SimpleForm>
			<m:VBox alignItems="Center">
				<m:Button id="createButton" text="Create" press="onCreate"/>
			</m:VBox>
		</m:content>
	</m:Page>
</mvc:View>