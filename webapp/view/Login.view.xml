<mvc:View
   controllerName="sap.ui.demo.fiori2.controller.Login"
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc">
	<semantic:SemanticPage id="loginPage">
		<Content>
			<Input
				value="val"
				description="Hello val"
				valueLiveUpdate="true"
				width="60%"/>
			<Button
				text="Say Hello"
				press=".onShowHello"/>
		</Content>
	</semantic:SemanticPage>
</mvc:View>